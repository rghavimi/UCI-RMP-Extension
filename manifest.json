{
  "manifest_version": 2,

  "name": "Rate My Professor",
  "description": "Helps students enroll",
  "version": "1.0",
  "background":{
      "scripts": ["popup.js"]
    },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
 

